{
  "private": true,
  "name": "thisischapman.com",
  "version": "1.1.0",
  "description": "The personal site of David Chapman, a web designer and front-end developer living and working in New York City, and living and breathing his passion beautifying the modern web. He's also probably breathing bus exhaust and that weird diaper smell coming from the trash can.",
  "author": "David Chapman <hello@thisischapman.com>",
  "license": "UNLICENSED",
  "homepage": "https://github.com/ronik-design/starter-template",
  "repository": "git://github.com/dchapman/thisischapman.com.git",
  "bugs": {
    "url": "https://github.com/ronik-design/starter-template.git/issues"
  },
  "engines": {
    "node": ">= 6.0.0"
  },
  "scripts": {
    "init": "tsg init",
    "clean": "rm -rf static/assets/javascripts && rm -rf static/assets/stylesheets && rm -rf static/assets/images && rm -f static/assets/*.json",
    "lint:js": "eslint src/javascripts",
    "lint:css": "stylelint src/stylesheets/**/*.scss --syntax scss",
    "lint": "npm-run-all -p lint:*",
    "imagemin": "imagemin src/images/* -o static/assets/images",
    "images:dev": "cp -R src/images static/assets/images",
    "serve": "node -e 'setTimeout(function() {}, 2000)' && browser-sync start --server build --port 5000 --files 'build/**/*, !build/sitemap.xml, !build/feed.xml' --reload-debounce",
    "tsg:build": "tsg build",
    "build:images": "npm run imagemin",
    "build:images:dev": "npm run images:dev",
    "build:webpack": "NODE_ENV=production webpack --display-error-details -p",
    "build:webpack:dev": "webpack --display-error-details -d",
    "build": "npm-run-all --serial clean build:images build:webpack",
    "deploy": "npm run build && tsg deploy",
    "watch:js": "webpack --watch",
    "watch:images": "chokidar 'src/images/**/*' -d -c 'npm run build:images'",
    "watch:tsg": "tsg watch",
    "watch": "npm-run-all --serial clean build:images:dev build:webpack:dev tsg:build && npm-run-all --continue-on-error --parallel watch:* serve",
    "start": "JFK_LOCAL=true npm run watch"
  },
  "devDependencies": {
    "autoprefixer": "^6.4.1",
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-0": "^6.5.0",
    "browser-sync": "^2.15.0",
    "chokidar-cli": "^1.2.0",
    "css-loader": "^0.26.1",
    "cssnano": "^3.7.4",
    "eslint": "^3.4.0",
    "eslint-config-xo-space": "^0.14.0",
    "eslint-plugin-babel": "^3.3.0",
    "extract-text-webpack-plugin": "2.0.0-beta.4",
    "favicons-webpack-plugin": "^0.0.7",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.3",
    "imagemin-cli": "^3.0.0",
    "imagemin-webpack-plugin": "^1.4.4",
    "json-loader": "^0.5.4",
    "json-manifest": "^1.0.1",
    "loader-utils": "^0.2.16",
    "lost": "^8.0.0",
    "mkdirp": "^0.5.1",
    "node-sass": "^3.9.3",
    "node-sass-import": "^1.0.5",
    "npm-run-all": "^3.1.0",
    "postcss-custom-media": "^5.0.1",
    "postcss-loader": "^1.2.1",
    "postcss-media-minmax": "^2.1.2",
    "postcss-urlrev": "^1.3.0",
    "sass-loader": "^4.1.0",
    "style-loader": "^0.13.1",
    "stylelint": "^7.2.0",
    "stylelint-config-standard": "^13.0.0",
    "svg-sprite-generator": "^0.0.7",
    "takeshape-cli": "^4.2.0",
    "webpack": "^3.5.5",
    "webpack-manifest-plugin": "^1.1.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "core-js": "^2.4.1",
    "get-parameter": "^1.0.8",
    "object-fit-images": "^2.3.1",
    "promise-polyfill": "^8.1.0",
    "slugg": "^1.2.1",
    "velocity-animate": "^1.5.2",
    "vue": "^2.4.4",
    "vue-loader": "^13.0.4",
    "vue-template-compiler": "^2.4.4",
    "whatwg-fetch": "^3.0.0"
  },
  "main": "index.js"
}
